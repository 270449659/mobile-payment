	 	color:#c0c0c0;
	 	line-height:25rpx;
}
.item-input .pwdinput{
	 	height:25rpx;
	 	font-size:24rpx;
	 	font-family:PingFang SC;
	 	font-weight:500;
	 	color:#c0c0c0;
	 	line-height:25rpx;
}
.item-input .userName-icon{
		 display: block;
		 width:17rpx ;
		 height: 24rpx;
		 line-height:24rpx;
		 margin-right:14rpx;
}
.item-input .userName-icon image{
	 	 width: 100%;
	 	 height:100%;
	 	 margin:0;
}
	/* 密码*/
.item-input.password{
		/* padding-left:140rpx; */
